{
  "extends": ["config:recommended"],
  "customManagers": [
      {
        "customType": "regex",
        "description": "Update GitHub release dependencies",
        "fileMatch": ["(^|/|\\.)conformance.*\\.yml$"],
        "matchStrings": [
          "#\\s*renovate:\\s*(datasource=(?<datasource>.*?)\\s*)?depName=(?<depName>.*?)$"
        ]
      }
    ]
}